<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.baemin.woowahan_presentation_android.search.SearchPresenterFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="24dp"
        android:descendantFocusability="beforeDescendants"
        android:focusableInTouchMode="true">

        <co.moonmonkeylabs.realmsearchview.RealmSearchView
            android:id="@+id/fragment_search_presenter_rsv"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:rsvHint="@string/search_hint"
            />
    </RelativeLayout>

</FrameLayout>
